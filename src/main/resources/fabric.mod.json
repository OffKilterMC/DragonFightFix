{
  "schemaVersion": 1,
  "id": "dragonfightfix",
  "version": "${version}",
  "name": "DragonFightFix",
  "description": "A simple mod to fix the issue where the dragon fight can get stuck if you quit the game while the Ender Dragon is making her dramatic entrance.",
  "authors": ["OffKilterMC"],
  "contact": {
    "sources": "https://github.com/offkiltermc/DragonFightFix",
    "issues": "https://github.com/offkiltermc/DragonFightFix/issues"
  },
  "license": "MIT",
  "icon": "assets/dragonfightfix/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "net.offkiltermc.dragonfightfix.client.DragonFightFixClient"
    ],
    "main": [
      "net.offkiltermc.dragonfightfix.DragonFightFix"
    ]
  },
  "mixins": [
    "dragonfightfix.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": ">=${minecraft_version}"
  }
}
